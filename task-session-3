<?php
// Define employee data array
$employees = [
    ["name" => "Ahmed", "salary" => 5000, "age" => 30, "insurance" => "Yes"],
    ["name" => "Sara", "salary" => 6000, "age" => 28, "insurance" => "No"],
    ["name" => "Ali", "salary" => 4500, "age" => 35, "insurance" => "Yes"]
];

echo '<h1>Employee Data</h1>';

// Loop through the employees and display data
foreach ($employees as $employee) {
    echo '<div style="border: 1px solid black; margin: 10px; padding: 10px;">';
    
    echo '<h3>Name: ' . $employee['name'] . '</h3>';
    echo '<div>Salary: ' . $employee['salary'] . '</div>';
    echo '<div>Age: ' . $employee['age'] . '</div>';
    echo '<div>Insurance: ' . $employee['insurance'] . '</div>';
    
    echo '</div>';
}
?>
